        <argsstring>(FLinearColor Color)</argsstring>
        <name>CreatePolygonSelection</name>
        <qualifiedname>URulerSelection::CreatePolygonSelection</qualifiedname>
        <param>
          <type><ref refid="_mass_l_o_d_types_8h_1a640a07441330729796f6bdaba973e7a8a6a061313d22e51e0f25b7cd4dc065233" kindref="member">FLinearColor</ref></type>
